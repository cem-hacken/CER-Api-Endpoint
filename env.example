# Exchange Data API - Environment Variables
# Copy this file to .env and update with your actual credentials

# API Security
API_KEY=[YOUR_API_KEY]

# Database Configuration (via VPN proxy)
DB_HOST=[VM_EXTERNAL_IP]
DB_PORT=5433
DB_NAME=[YOUR_DB_NAME]
DB_USER=[YOUR_DB_USERNAME]
DB_PASSWORD=[YOUR_DB_PASSWORD]

# Optional: Google Cloud Project (for Secret Manager)
# GOOGLE_CLOUD_PROJECT=[YOUR_PROJECT_ID] 